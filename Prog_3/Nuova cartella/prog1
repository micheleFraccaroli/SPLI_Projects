# lo will be used to TX and RX 
# 0 A:1
# 1 A:2
# 2 B
0	i	0	lo
0	i	1	lo
0	i	2	lo
0	P	0	0x10010
0	P	1	0x10011
0	P	2	0x10012
0	S	0	1000
0	S	1	1000
0	S	2	1000
# start all flows
0	R	0	900000
0	R	1	900000
0	R	2	900000
#change 1
20000    R      0        20k
#change 2
30000    R      2        30k
# stop 0
40000	X	0	0
